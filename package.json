{
  "name": "my-homepage",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "datamaps": "^0.5.9",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "scripts": {
    "start": "npx webpack-dev-server --watch --mode development",
    "clean": "rm -rf build/*",
    "copy-assets": "mkdir -p build && cp -r public/* build",
    "test": "mocha",
    "bundle": "npx webpack",
    "deploy-backend": "./bin/deploy_backend.sh",
    "build": "npm test && npm run copy-assets && npm run bundle && cp index.html build && cp error.html build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-react-app": "^3.1.2",
    "css-loader": "^1.0.0",
    "mocha": "^5.2.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
